name "keepup"
targetType "none"
description "A command-line task manager"
authors "araspik"
copyright "Copyright Â© 2018, araspik"
license "MIT"

dependency "keepup:library" version="*"
dependency "keepup:executable" version="*"

subPackage {
  name "library"
  targetType "library"
  dependency "sdlang-d" version="~>0.10.4"
  sourcePaths "source"
}

subPackage {
  name "executable"
  targetType "executable"
  dependency "keepup:library" version="*"
  sourcePaths "src/keepup"
  importPaths "src"
}
